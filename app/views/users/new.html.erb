<div class="row">
    <div class="large-6 columns">

<%= provide(:title, "Signup")  %>
<%= form_for(@user)  do |f| %>
<%= render 'shared/error_messages' %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :email %>
<%= f.email_field :email%>
<%= f.label :password%>
<%= f.password_field :password %>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
 <%# <%= recaptcha_tags %1> %>
<%= f.submit "SignUp"%>
<% end %>
    </div>
    </div>
